                >\r\n                        Logout\r\n                      </Link>\r\n                    </div>\r\n                  </div>\r\n                ) \r\n                : \r\n                (\r\n                  <>\r\n                    <Link to=\"/register\">Register</Link>\r\n                    <Link to=\"/login\">Login</Link>\r\n                  </>\r\n                )}\r\n\r\n                <Link to=\"/cart\">\r\n                  <i className=\"fas fa-shopping-bag\"></i>\r\n                  <span className=\"badge\">{cartItems.length}</span>\r\n                </Link>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Header;\r\n","import React from \"react\";\r\n\r\nconst Rating = ({ value, text }) => {\r\n  return (\r\n    <div className=\"rating\">\r\n      <i\r\n        className={\r\n          value >= 1\r\n            ? \"fas fa-star\"\r\n            : value >= 0.5\r\n            ? \"fas fa-star-half-alt\"\r\n            : \"far fa-star\"\r\n        }\r\n      ></i>\r\n      <i\r\n        className={\r\n          value >= 2\r\n            ? \"fas fa-star\"\r\n            : value >= 1.5\r\n            ? \"fas fa-star-half-alt\"\r\n            : \"far fa-star\"\r\n        }\r\n      ></i>\r\n      <i\r\n        className={\r\n          value >= 3\r\n            ? \"fas fa-star\"\r\n            : value >= 2.5\r\n            ? \"fas fa-star-half-alt\"\r\n            : \"far fa-star\"\r\n        }\r\n      ></i>\r\n      <i\r\n        className={\r\n          value >= 4\r\n            ? \"fas fa-star\"\r\n            : value >= 3.5\r\n            ? \"fas fa-star-half-alt\"\r\n            : \"far fa-star\"\r\n        }\r\n      ></i>\r\n      <i\r\n        className={\r\n          value >= 5\r\n            ? \"fas fa-star\"\r\n            : value >= 4.5\r\n            ? \"fas fa-star-half-alt\"\r\n            : \"far fa-star\"\r\n        }\r